{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true,
    "quotemark": [true, "single", "jsx-double"],
  },
  "rulesDirectory": []
}
